
#remove php file extension-e.g. https://example.com/file.php will become https://example.com/file 
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L] 

ErrorDocument 404 http://ihub.himilouniversity.edu.so/404
ErrorDocument 500 http:///ihub.himilouniversity.edu.so/500
ErrorDocument 503 http://ihub.himilouniversity.edu.so/503

